#include <iostream>
using namespace std;
int findMax(const int arr[][100], int n, int m);
int main() {
    int arr[100][100];
    int n, m;
    cout << "Enter number of rows and columns of matrix(n m): ";
    cin >> n >> m;
    cout << "Enter elements of matrix: ";
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < m; j++) {
            cin >> arr[i][j];
        }
    }
    cout<<"Maximum element is: "<< findMax(arr, n, m);
}
int findMax(const int arr[][100], int n, int m) {
    int max = 0;
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < m; j++) {
            if (arr[i][j] > max) {
                max = arr[i][j];
            }
        }
    }
    return max;
}
